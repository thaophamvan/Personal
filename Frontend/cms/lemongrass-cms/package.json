{
  "name": "lemongrass",
  "version": "12.1.0",
  "description": "Content management system build on express.js and Cloudant DB",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "local": "set NODE_ENV=Local&&node app.js",
    "dev": "set NODE_ENV=Dev&&node app.js",
    "stage": "set NODE_ENV=Stage&&node app.js",
    "prod": "set NODE_ENV=Prod&&node app.js",
    "watch": "webpack --watch --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build:debug": "webpack --config webpack/webpack.debug.config.js  --mode development",
    "build:dev": "webpack --config webpack/webpack.dev.config  --mode development",
    "build:stage": "webpack --config webpack/webpack.stage.config --mode none",
    "build:prod": "webpack --config webpack/webpack.prod.config --mode production",
    "lint": "eslint **/*.js --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint-client": "eslint --fix --ext .js client/",
    "eslint-server": "eslint --fix --ext .js server/",
    "git-tag-create:dev": "git tag -a DEV_v.10.2.0 -m \"patch version DEV_v.10.2.0\"",
    "git-tag-create:prod": "git tag -a PROD_v.0.0.1 -m \"patch version PROD_v.0.0.1\"",
    "git-tag-create:stage": "git tag -a RELEASE_v.11.0.1 -m \"patch version RELEASE_v.11.0.1\"",
    "git-push-tag:dev": "git push origin DEV_v.10.2.0",
    "git-push-tag:stage": "git push origin RELEASE_v.0.1.3",
    "git-push-tag:prod": "git push origin PROD_v.0.1.3",
    "new-minor-version": "npm version minor",
    "new-major-version": "npm version major",
    "new-patch-version": "npm version patch",
    "new-prepatch-version": "npm version prepatch",
    "new-preminor-version": "npm version preminor",
    "new-premajor-version": "npm version premajor"
  },
  "keywords": [
    "web",
    "app",
    "cms",
    "adim",
    "framework"
  ],
  "author": "Personal",
