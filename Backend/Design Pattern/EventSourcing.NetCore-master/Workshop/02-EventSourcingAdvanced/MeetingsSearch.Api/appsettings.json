{
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "AllowedHosts": "*",

    "elasticsearch": {
        "index": "meetings",
        "url": "http://localhost:9200/"
    },

    "KafkaConsumer": {
        "ConsumerConfig": {
            "GroupId": "MeetingsSearch",
            "BootstrapServers": "localhost:9092",
            "AutoOffsetReset": "Earliest"
        },
        "Topics": [ "MeetingsManagement" ]
    },

    "KafkaProducer": {
        "ConsumerConfig": {
            "BootstrapServers": "localhost:9092"
        },
        "Topic": "MeetingsSearch"
    }
}
